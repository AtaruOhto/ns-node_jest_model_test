{
  "dependencies": {
    "bcrypt": "^2.0.1",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "mysql2": "^1.5.3",
    "sequelize": "^4.37.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/express": "^4.11.1",
    "@types/faker": "^4.1.2",
    "@types/jest": "^22.2.3",
    "@types/sequelize": "^4.27.14",
    "jest": "^22.4.3",
    "sequelize-cli": "^4.0.0",
    "ts-jest": "^22.4.4",
    "ts-node": "^6.0.0",
    "tsc": "^1.20150623.0",
    "tslib": "^1.9.0",
    "typescript": "^2.8.1"
  },
  "scripts": {
    "test": "env NODE_ENV=test jest --forceExit"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^.+\\.ts$": "<rootDir>/spec/jest/typescriptTransform.js"
    },
    "testMatch": [
      "<rootDir>/spec/**/?(*.)(spec).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "absolute_path_to_your_ts_config_json"
      }
    }
  }
}
